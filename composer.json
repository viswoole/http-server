{
  "name": "viswoole/http-server",
  "description": "ViSwoole HttpServer 是一个基于 Swoole 的高性能 HTTP 服务器。",
  "keywords": [
    "http",
    "server",
    "viswoole"
  ],
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "zhuchonglin",
      "email": "8210856@qq.com"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-swoole": ">=5.1",
    "ext-fileinfo": "*",
    "viswoole/core": "@dev",
    "psr/http-message": ">=2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "ViSwoole\\HttpServer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ViSwoole\\HttpServer\\Tests\\": "tests"
    }
  },
  "extra": {
    "viswoole": {
      "services": [
        "\\ViSwoole\\HttpServer\\RouterService"
      ],
      "configs": [
        "config"
      ]
    }
  }
}
